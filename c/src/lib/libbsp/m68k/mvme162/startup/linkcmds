MEMORY
        {
        ram : org = 0x100000, l = 1M
        }

SECTIONS
{
        .text 0x100000 :
        {
        text_start = . ;
        *(.text)
        etext = ALIGN( 0x10 ) ;
        }
        .data ADDR( .text ) + SIZEOF( .text ):
        {
        data_start = . ;
        *(.data)
        edata = ALIGN( 0x10 ) ;
        }
        .bss ADDR( .data ) + SIZEOF( .data ):
        {
        bss_start = . ;
        *(.bss)
        *(COMMON)
        end = . ;
        _end = . ;
        }
}
